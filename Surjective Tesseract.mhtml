    (* Hasse *)
    Graph[gR, VertexLabels -&gt; gV, VertexCoordinates -&gt; gC], (* else *)
    Print["Sets to mark up: ", zevS, ". Draw an augmented Hasse diagram."];
    (* Hasse augmented *)
    gH = s2v[n, #]&amp; /@ zevS;
    Graph[gR, VertexLabels -&gt; gV, VertexCoordinates -&gt; gC, GraphHighlight -&gt; gH]
    ]
